<% # To see the raw XML that's parsed below:
   # http://vetiver.cc.columbia.edu:7014/vxws/GetHoldingsService?bibId=2016492
 %>

<%- holdings = add_display_elements(@holdings['condensed_holdings_full']).listify.reject { |x| x['location_name'].to_s.strip == "Online"} %>
<%- unless holdings.empty? %>
Holdings:

<%- holdings.each do |entry| %>
<%= "#{entry['location_name']} #{entry['hours']}" %>
  <%= entry['call_number'] %>
<%- entry['copies'].each do |copy| %>
  <%- copy['temp_locations'].listify.each do |tl| %>
  <%= tl %>
  <%- end %>
  <%- ['orders', 'summary_holdings', 'supplements', 'indexes', 'notes', 'reproduction_note', 'current_issues'].collect { |type| copy[type]}.compact.each do |copy_type| %>
  <%= copy_type %>
  <%- end %>
  <%- copy['items'].each do |item| %>
  <%- message, details = item %>
  <%= message %>
    <%- if details['count'] >1 %>
  <%="(#{details['count']} copies)" %>
    <%- end %>
  <%- end %>
<%- end %>

<%- end %>
<%- end %>
